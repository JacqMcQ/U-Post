<!-- views/homepage.handlebars -->

<section class="container">
  <h1>Blog List</h1>
  <section class="flex-row justify-center">
    {{#each blogs}}
      <div class="blog-post">
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <p>Posted by: {{this.user.username}}</p>
      </div>
    {{/each}}
  </section>
</section>
